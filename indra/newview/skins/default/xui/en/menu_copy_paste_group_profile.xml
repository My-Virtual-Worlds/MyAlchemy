   label="Copy Group Name"
   layout="topleft"
   name="copy_group_name">
    <menu_item_call.on_click
     function="Profile.Commit"
     parameter="copy_group_name"/>
  </menu_item_call>
  <menu_item_call
   label="Copy SLurl"
   layout="topleft"
   name="copy_slurl">
    <menu_item_call.on_click
