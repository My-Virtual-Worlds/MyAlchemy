<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 layout="topleft"
 follows="top|right|left"
 width="350"
 height="66"
 name="music_ticker"
 class="music_ticker">
    <string name="paused">
        (not playing)
    </string>
    <string name="loading">
